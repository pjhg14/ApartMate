                            <Menu text="Edit">
                                <MenuItem text="TODO"/>
                            </Menu>
                        </MenuBar>
                        <HBox spacing="10.0">
                            <children>
                                <Button fx:id="backButton" mnemonicParsing="false" onAction="#backToApartment"
                                        text="Previous Page"/>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tenants">
                                    <font>
