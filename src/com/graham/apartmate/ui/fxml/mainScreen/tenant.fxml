                            </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                            <children>
                                <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                                    <children>
                                        <VBox fx:id="cell1" alignment="CENTER">
                                            <children>
                                                <ImageView fx:id="image1" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../../res/Tenantimg_small.png"/>
                                                    </image>
                                                </ImageView>
                                                <Text fx:id="name1" strokeType="OUTSIDE" strokeWidth="0.0" text="Text"/>
