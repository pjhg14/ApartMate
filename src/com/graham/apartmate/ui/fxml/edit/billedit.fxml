<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="351.0" prefWidth="460.0" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.graham.apartmate.ui.editwindows.bill.BillEditController">
    <children>
        <VBox alignment="CENTER" layoutX="83.0" layoutY="59.0" prefHeight="234.0" prefWidth="225.0" spacing="20.0">
            <children>
                <TextField fx:id="nameTextField" promptText="Name"/>
                <TextField fx:id="addressTextField" promptText="Address"/>
                <TextField fx:id="billTextField" promptText="Bill"/>
                <TextField fx:id="phoneTextField" promptText="Phone #"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </VBox>
        <Button defaultButton="true" layoutX="398.0" layoutY="302.0" mnemonicParsing="false"
                onAction="#editBill" text="Add"/>
        <Button cancelButton="true" layoutX="317.0" layoutY="302.0" mnemonicParsing="false" onAction="#closeWindow"
                text="Cancel"/>
    </children>
</AnchorPane>